{
  "name": "transformime-marked",
  "version": "0.0.1",
  "description": "Transforms Markdown to HTML Elements, adhering to the transformime interface",
  "main": "lib/marked.transform.js",
  "scripts": {
    "prebuild": "mkdirp lib && mkdirp dist && npm run clean",
    "test": "mocha --compilers js:babel/register",
    "build": "npm run build:es5 && npm run build:browser",
    "build:es5": "babel src --out-dir lib/ --source-maps",
    "build:browser": "browserify lib/*.js --outfile dist/tf-marked.js",
    "prepublish": "npm run build",
    "watch": "parallelshell 'npm run watch:test -s' 'npm run watch:build -s'",
    "watch:build": "nodemon -q -w src/ --ext '.' --exec 'npm run build'",
    "watch:test": "npm run test -- -w",
    "clean": "rimraf lib/* dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nteract/transformime-marked.git"
  },
  "keywords": [
    "transform",
    "mime",
    "mimetype",
    "markdown",
    "jupyter"
  ],
  "author": "nteract",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nteract/transformime-marked/issues"
  },
  "homepage": "https://github.com/nteract/transformime-marked",
  "devDependencies": {
    "babel": "^5.6.14",
    "browserify": "^10.2.6",
    "chai": "^3.2.0",
    "jsdom": "^5.6.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "nodemon": "^1.3.7",
    "parallelshell": "^1.2.0",
    "rimraf": "^2.4.1"
  },
  "dependencies": {
    "marked": "0.3.5"
  }

,"_resolved": "https://registry.npmjs.org/transformime-marked/-/transformime-marked-0.0.1.tgz"
,"_integrity": "sha1-11IR5bBcs5c16kjbgY9ZO519RO0="
,"_from": "transformime-marked@0.0.1"
}